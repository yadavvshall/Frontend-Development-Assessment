	pauseAudio();
	$("#TabPopup5 #popup").show();
	$("#TabPopup5 .sectionTop").addClass("blur");
	$("#TabPopup5 .sectionContent").addClass("blur");

});
$(".popupclose").on("click", function() {
	playAudio();
	$("#TabPopup5 #popup").removeAttr("style");
	$("#TabPopup5 .sectionTop").removeClass("blur");
	$("#TabPopup5 .sectionContent").removeClass("blur");
});
function pauseAudio() {
    if (!fnPauseAudio()) {
        console.log(true);
        fnPauseAudio()
    } else {
        playPauseAudio();
        console.log(false);
    }
}

function playAudio() {
    if (!fnPauseAudio()) {
        console.log(true);
        playPauseAudio();
    }
}
</script>

