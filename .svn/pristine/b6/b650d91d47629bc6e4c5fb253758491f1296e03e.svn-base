            }
            if (visitedArr.length == 3) {
                enableNextBtn();
                fnCompletion();
                fnDesiableNext(false);                    
                $('.boxInst').show();
            }
          
        } else {
            $("#idTick0" + selectedIndex).css({
                "opacity": "1"
            });
            $("#clsTabTxt" + selectedIndex).css({
                'color': '#80848b'
            });
            $(".ClsTab").removeClass("clsDisable");
            // $("#TabImage"+selectedIndex).addClass("clsDisable");
            fnCompletion();             
        }
    }
    function endModule() {
        var _getCount = __tabIndex + __indexTab + __clickIndex + __indexClick;
        console.log("total count" + _getCount);
        if (_getCount >= 4 || gCompPageArr.indexOf("M1L0T1P14") != -1) {
            console.log("inside");
            $('.boxInst').show();
            setTimeout(function () {
                fnGlowNextBtn();
                enableNextBtn();                  
            }, 500)
        }
    }
    function pauseAudio() {
        if (!fnPauseAudio()) {
            console.log(true);
            fnPauseAudio()
        } else {
            playPauseAudio();               
        }
    }
    function playAudio() {
        if (!fnPauseAudio()) {            
            playPauseAudio();
        }
    }
   
</script>   
