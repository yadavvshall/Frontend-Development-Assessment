$('#topImgCnt2').hide();
function submitClick() {
    tinCanWrapper.sendStatementData("answered", "baby-jazmine-lab-results-at-2-hours-classification-of-blood-sugar", "LAB RESULTS (Obtained at 2 hours of age) Blood sugar: 32 mg/dL (1.8 mmol/L), CBC results: Pending, Capillary blood gas: pH:7.11, PCO2:55, PO2:30, HCO3:17, Base:-13, mmHg:(55,30), kPa:(7.3,4). How would you classify Baby Jazmine’s blood sugar?");
    $('.cSubmitBtn.active').hide();
    console.log("clicked");
    enableNextBtn();
    fnDesiableNext(false);
    gCompPageArr.push("M1L0T1P29");
    var incorrectCount = 0; //, correctCount = 0;
    $('.option').each(function() {
        if ($(this).hasClass('clicked') && ($.inArray($(this).index(), correctOptions) == -1)) {
            $(this).addClass('wrong');
            incorrectCount++;
        }
        $(this).off(); //.removeClass('clicked');
    });

    $(correctOptions).each(function(i, val) {
        $('.option').eq(val).addClass('correct');
    });

    if (incorrectCount == 0) {
        fnPlaySectionAudio(0);
    } else {
        fnPlaySectionAudio(1);
    }
    $('.option').off().removeClass('aactive'); //.css({'cursor':'default'});
    $('.cSubmitBtn').removeClass('active');
    $('.informationRight').show();
}

