<script language="javascript" type="text/javascript">
	
	
	$(document).ready(function () {	
	
	/* $("#clsLinksClose").click(function(){			
			disableHelpPopup();  
	   }); 		*/
        
        // Below lines are added for 508 accessibility.
        var arrTabIndexUpdate = ['#id_whiteSheenDiv'];
        updateTabIndex(arrTabIndexUpdate, '', true, "#id_whiteSheenDiv");  
		fnClickLink();
	 });
	  var popUpOpen = false;
	
	 function fnClickLink(){
			$(".clsLink").removeClass("clsDisable");
			$(".clsLink").off().not(".clsDisable").on("click",function(){
					popUpOpen = true;
				var url = $(this).attr("data-url");
				var isLink = $(this).attr("isLink");
				if(isLink=="true"){
					window.open(url,"_blank");
				}
			});
			fnCloseLinkPopup();
		}
	 function fnCloseLinkPopup(){
			$("#id_whiteSheenDiv,.clsLinksClose").off("click").on("click", closePopup);
		}
		function closePopup(){
				
				
			if(!popUpOpen){
				$("#id_whiteSheenDiv").css({"display":"none"});
				disableHelpPopup(); 
				fnPlayAudio();
				
				
			}
			
			popUpOpen = false;
			
		}
	 
	 
</script>

