                    gAudioId = "";

                    fnPlaySectionAudio(4);

                }

                tinCanWrapper.sendStatementData("answered", "Umbilical-Catheters-Indications-Identify-the-reasons-for-using-a-UVC-and/or-a-UAC-by...", "Umbilical Catheters - Indications - Identify the reasons for using a UVC and/or a UAC by clicking on the drop-down menus and selecting the appropriate option.");

                actfirst = 1;

            } else if (curId == 2) {

                //$('.feedbox').removeClass('feedbackContainer1').removeClass('feedbackContainer3').addClass('feedbackContainer2');

                if (incorrectCount != 0) {

                    $('.feedTitle strong').html('Here is some feedback on your response.');

                    gAudioId = "";

                    fnPlaySectionAudio(7);



                } else {

                    gAudioId = "";

                    fnPlaySectionAudio(6);

                }

                tinCanWrapper.sendStatementData("answered", "Umbilical-Catheters-Medications-Which-umbilical-catheter-should-be-used-for-the-following-items?", "Umbilical Catheters - Medications - Which umbilical catheter should be used for the following items?");

                actsecond = 1;

            } else if (curId == 3) {

                //$('.feedbox').removeClass('feedbackContainer1').removeClass('feedbackContainer2').addClass('feedbackContainer3');

                if (incorrectCount != 0) {

                    $('.feedTitle strong').html('Here is some feedback on your response.');

                    gAudioId = "";

                    fnPlaySectionAudio(9);



                } else {

                    gAudioId = "";

                    fnPlaySectionAudio(8);



                }

                actthird = 1;

            }

            $('.tick_' + curId).show();

            // $('.mainContainer').addClass('blur')



            $('.cSubmitBtn').removeClass('active').hide();

            enableNextFunc();





        }

    } else {

        //debugger;

        attemptCount1++;

        $("select._options").each(function() {

            if ($(this).parent().hasClass('incorrect')) {

                $(this).css("pointer-events", "all");

            }

            $(this).parent().removeClass('incorrect');

            $(this).parent().removeClass('correct');

            $(this).css("pointer-events", "all");

            $(this).val(0).change();



        });

        $(e.target).html('Submit');

    }





}


