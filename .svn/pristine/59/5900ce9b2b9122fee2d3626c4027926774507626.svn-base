            // $(".answerChoices .listcnt").addClass("disabled");
            disableNextBtn();
            // $('.feedbackTxt').hide();
            var ele = document.getElementsByTagName('div');
            for (var i = 0; i < ele.length; i++) {
                ele[i].setAttribute("unselectable", "on");
            }
            var n = isInArray("M1L0T1P19", gCompPageArr);
            if (n == true) {
                enableNextBtn();
                // fnGlowNextBtn();
                fnDesiableNext(false);
            }

        });

        function audioReset() {
            gAudioId = "";
            onTimelineEnd();
        }

        function isInArray(value, array) {
            return array.indexOf(value) > -1;
        }

        function fnPlaySectionAudio(gCurrentIndex) {
            indexAudio = (gCurrentIndex);
            audioId = "audio_" + gCurrentIndex;
            gAudioId = audioId;
            fnHideShowAudioPlayer(false);
            updateContentArray = eval("popupSectionArray" + (gCurrentIndex));
            initAudioPlayer(gItemAudioPath[gCurrentIndex])
        }

        var gTotalPopups = 2;
        var temp = "";

        function fnClick() {
            $(".ClsTab").off("click").on("click", function() {
                audioReset();
                $(".boxInst").hide();
                $("answerChoices .OptionCLSS").removeClass("correctAnswer").removeClass("wrongAnswer");
            });
        }

        function fnCompletion() {
            enableNextBtn();
            fnAudioFinishItemComp();
            $(".boxInst").show();
            //gCompPageArr.push("M1L0T1P19");
            audioStop();
        }

        function fnOnAudioComplete() {

            if (gAudioId == "startAudio") {
                fnClick();
                $(".answerChoices .listcnt").removeClass("disabled");
            } else {
                if (qAttempts == 0) {
                    $(".answerChoices .listcnt").removeClass("disabled");
                }

                if (qAttempts == 1 && isCorrect == false) {
                    $(".answerChoices .listcnt").removeClass("disabled");
                    $('.clicked').removeClass('wrongAnswer');
                    $('.clicked').removeClass("clicked").removeAttr("clicked");
                } else {
                    $(".answerChoices .listcnt").addClass("disabled");
                    if (qAttempts == 2 || isCorrect == true) {
                        fnCompletion();
                    }
                }

            }
        }


        function interSect(a, b) {
            return $.grep(a, function(i) {
                return $.inArray(i, b) > -1;
            });
        }



        function pauseAudio() {
            if (!fnPauseAudio()) {
                console.log(true);
                fnPauseAudio()
            } else {
                playPauseAudio();
                console.log(false);
            }
        }

        function playAudio() {
            if (!fnPauseAudio()) {
                console.log(true);
                playPauseAudio();
            }
        }

        $(document).off("click").on("click", '.answerChoices .listcnt', function(event) {
            if (!$(this).hasClass("disabled")) {
                event.preventDefault();
                //audioReset();
                if ($(this).attr("clicked") == "true") {
                    $(this).removeClass("clicked").removeAttr("clicked");
                    $(".cSubmitBtn").removeClass("active");
                    //disableSubmitBtn(".cSubmitBtn");
                } else {
                    $('.clicked').removeClass("clicked").removeAttr("clicked");
                    $(this).addClass("clicked").attr("clicked", "true");
                    $(".cSubmitBtn").removeClass("active").addClass("active");
                    // enableSubmitBtn(".cSubmitBtn");
                }
            }
        });

        $(".cSubmitBtn").on("click", function() {
            if ($(this).hasClass("active")) {
                // fnCompletion()                

                // fnGlowNextBtn();

                qAttempts++;
                // $(".answerChoices .listcnt").addClass("disabled");
                fnPlaySectionAudio(2);
                if ($('.clicked').attr('curid') == 2) {
                    tinCanWrapper.sendStatementData("answered", "knowledge-check-question-1", "A mother presents in labor at 37 weeks gestation to a community hospital. The admission history and physical exam reveal a complaint of itching and pain in the vaginal area. Inspection reveals a cluster of lesions suspicious for herpes simplex viral (HSV) infection. The mother delivers precipitously in bed. The infant is born with 3 small vesicles on the scalp but is otherwise doing well. Your thought process about this infant is:");
                    $('.clicked').addClass('correctAnswer');
                    $(this).hide();
                    isCorrect = true;
                    enableNextBtn();
                    fnDesiableNext(false);
                    volumeProgressState(false);
                    gCompPageArr.push("M1L0T1P19");
                    $(".answerChoices .listcnt").addClass("disabled");

                } else {
                    tinCanWrapper.sendStatementData("answered", "knowledge-check-question-1", "A mother presents in labor at 37 weeks gestation to a community hospital. The admission history and physical exam reveal a complaint of itching and pain in the vaginal area. Inspection reveals a cluster of lesions suspicious for herpes simplex viral (HSV) infection. The mother delivers precipitously in bed. The infant is born with 3 small vesicles on the scalp but is otherwise doing well. Your thought process about this infant is:");
                    $('.clicked').addClass('wrongAnswer');
                    $('#showList_2').addClass('correctAnswer');
                    isCorrect = true;
                    enableNextBtn();
                    fnDesiableNext(false);
                    volumeProgressState(false);
                    gCompPageArr.push("M1L0T1P19");

                    $(this).hide();
                    fnPlaySectionAudio(1);
                    $(".answerChoices .listcnt").addClass("disabled");

                }

            } else {

            }
        });
        
    </script>

