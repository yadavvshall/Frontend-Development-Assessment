            show: "",
            hide: "",
            transcript: "Here is some feedback on your response. Exposure to HSV at birth increases the risk for disseminated herpes infection including central nervous system involvement. <br>Click Next to continue."
        }];
       
		var popupSectionArray2 = [{
            time: "0",
            carry: "",
            event: "",
            show: "",
            hide: "",
