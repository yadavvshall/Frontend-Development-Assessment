    <script>
        var audioPath = "pages/audio/Topic01/T01P19/T01P19";
        var gItemAudioPath = ["pages/audio/Topic01/T01P19/T01P19_1", "pages/audio/Topic01/T01P19/T01P19_1", "pages/audio/Topic01/T01P19/T01P19_2"];
        var qAttempts = 0,
            isCorrect = false;
        var currCue = 0;
