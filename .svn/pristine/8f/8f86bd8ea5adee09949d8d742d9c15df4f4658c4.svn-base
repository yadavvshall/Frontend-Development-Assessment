        }



        $(document).ready(function() {

            if ($(".listcnt").hasClass('clicked')) {
                $(".cSubmitBtn").addClass("active");
            }

            /* $('#clsMnuBtn, #clsReplay, #clsBackBtn, #ToolBtn').css({
                "pointer-events": "none"
            }); */

            backBtnDisableEnableForReview();

            $(".answerChoices .listcnt").addClass("disabled");
            $("#clsPlayPauseBtn").hide();

            disableNextBtn();

            $('.information').on("click", function() {

                // $("").css("display", "block");

                $('.mainContainer').addClass('blur');

                $('.activityArea').addClass('blur');

                $('.informationInstruction').addClass('blur');

                $('.cSubmitBtn').addClass('blur');

                //debugger;

                $(".informationOveryplay").fadeIn(300, function() {

                    $(".informationPopup").fadeIn(200);

                });





            });

            $(".informationOveryplay").on("click", function() {

                //  $(".informationOveryplay").css("display", "none");

                // $(".informationPopup").css("display", "none");

                $(".informationPopup").fadeOut(200);

                $(".informationOveryplay").fadeOut(300, function() {

                    $('.mainContainer').removeClass('blur');

                    $('.activityArea').removeClass('blur');

                    $('.informationInstruction').removeClass('blur');

                    $('.cSubmitBtn').removeClass('blur');

                });

            });

            $(".headingIcon").on("click", function() {



                $(".informationPopup").fadeOut(200);

                $(".informationOveryplay").fadeOut(300, function() {

                    $('.mainContainer').removeClass('blur');

                    $('.activityArea').removeClass('blur');

                    $('.informationInstruction').removeClass('blur');

                    $('.cSubmitBtn').removeClass('blur');

                });



            });



            var ele = document.getElementsByTagName('div');

            for (var i = 0; i < ele.length; i++) {

                ele[i].setAttribute("unselectable", "on");

            }

            var n = isInArray("M1L0T1P59_2", gCompPageArr);

            if (n == true) {

                var _getSelectedOpt = assessmentObj.getSelectedOptionId(1);
                assessmentObj.checkedAnswerFunc(1, ".listcnt[curid='" + _getSelectedOpt + "']");
                enableNextBtn();
                //fnGlowNextBtn();
                fnDesiableNext(false);

                if ($('.clicked').attr('curid') == 2) {
                    //correct_answer--;
                    // correct_answer++
                    // correct_answer.push($('.clicked').attr('curid'));
                    //console.log("85 correct " + _getcorrect);

                    fnCompletion();

                    isCorrect = true;
                    $(".answerChoices .listcnt").addClass("disabled");

                    $(".cSubmitBtn").removeClass("active");

                } else {
                    //incorrect_answer--

                    $(".cSubmitBtn").removeClass("active");

                    fnCompletion();

                }

            }

            fnClick();

            $(".answerChoices .listcnt").removeClass("disabled");


        });



        function audioReset() {

            gAudioId = "";

            onTimelineEnd();

        }



        function isInArray(value, array) {

            return array.indexOf(value) > -1;

        }



        function fnPlaySectionAudio(gCurrentIndex) {

            indexAudio = (gCurrentIndex);

            audioId = "audio_" + gCurrentIndex;

            gAudioId = audioId;

            fnHideShowAudioPlayer(false);

            updateContentArray = eval("popupSectionArray" + (gCurrentIndex));

            initAudioPlayer(gItemAudioPath[gCurrentIndex])

        }



        var gTotalPopups = 2;

        var temp = "";



        function fnClick() {

            $(".ClsTab").off("click").on("click", function() {

                audioReset();

                $(".boxInst").hide();

                $("answerChoices .OptionCLSS").removeClass("correctAnswer").removeClass("wrongAnswer");



                //}

            });

        }



        function fnCompletion() {

            enableNextBtn();

            fnAudioFinishItemComp();



            $(".boxInst").show();

            gCompPageArr.push("M1L0T1P59_2");

        }





        function fnOnAudioComplete() {



            if (gAudioId == "startAudio") {

                $(".ClsTab").removeClass("clsDisable");

                // we need to replace this

                //enableNextBtn();

                //fnAudioFinishItemComp();

                // we need to replace this



            } else {

                if (qAttempts == 0) {
                    $(".answerChoices .listcnt").removeClass("disabled");
                }
                if (qAttempts == 1 && isCorrect == false) {
                    $(".answerChoices .listcnt").removeClass("disabled");
                    $('.clicked').removeClass('wrongAnswer');
                    $('.clicked').removeClass("clicked").removeAttr("clicked");
                    //		$('.feedbackTxt1').hide();
                } else {
                    $(".answerChoices .listcnt").addClass("disabled");
                    if (qAttempts == 2 || isCorrect == true) {
                        fnCompletion();
                    }
                }
            }
        }


        function pauseAudio() {

            if (!fnPauseAudio()) {

                fnPauseAudio()

            } else {

                playPauseAudio();

            }

        }



        function playAudio() {

            if (!fnPauseAudio()) {

                playPauseAudio();

            }

        }



        $(document).off("click").on("click", '.answerChoices .listcnt', function(event) {

            if (!$(this).hasClass("disabled")) {

                event.preventDefault();

                //audioReset();

                if ($(this).attr("clicked") == "true") {

                    $(this).removeClass("clicked").removeAttr("clicked");

                    $(".cSubmitBtn").removeClass("active");

                    //disableSubmitBtn(".cSubmitBtn");



                } else {

                    $('.clicked').removeClass("clicked").removeAttr("clicked");

                    $(this).addClass("clicked").attr("clicked", "true");

                    $(".cSubmitBtn").removeClass("active").addClass("active");

                    // enableSubmitBtn(".cSubmitBtn");



                }

            }



        });


        $(".cSubmitBtn").on("click", function() {
            _clicked = $('.clicked').attr('curid')
                //console.log(clickedQuestionValue)
            assessmentObj.clickedQuestionValue(_clicked);
            volumeProgressState(false);
            // gCompPageArr.push("M1L0T1P59_2");

            if ($(this).hasClass("active")) {

                tinCanWrapper.sendStatementData("answered", "A-5-kilogram-38-week-gestation-infant-was-born-to-a-39-year-old-gravida-4-mother-who-required-insulin-for-management-of-her-gestational-diabetes.The-infant-requires...", "A 5 kilogram 38-week gestation infant was born to a 39-year-old gravida 4 mother who required insulin for management of her gestational diabetes. The infant requires intravenous dextrose to maintain the blood sugar in a normal range. A new graduate nurse asks you to explain why this infant is hypoglycemic.");

                $(".answerChoices .listcnt").addClass("disabled");

                if ($('.clicked').attr('curid') == 2) {
                    assessmentObj.correctAnswerIncrement();
                    fnCompletion();
                    isCorrect = true;
                    $(".answerChoices .listcnt").addClass("disabled");
                    $(".cSubmitBtn").removeClass("active");

                } else {
                    //incorrect_answer++
                    assessmentObj.wrongAnswerIncrement();
                    $(".cSubmitBtn").removeClass("active");
                    fnCompletion();
                }

                fnJumpToPageByID("M1L0T1P59_3");


            } else {



            }

        });
    </script>



