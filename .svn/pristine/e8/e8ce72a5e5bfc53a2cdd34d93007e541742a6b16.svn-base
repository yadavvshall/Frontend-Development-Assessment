.blur {

    -webkit-backface-visibility: hidden;

    -moz-backface-visibility: hidden;

    -ms-backface-visibility: hidden;

    -o-backface-visibility: hidden;

    -webkit-filter: blur(12px);

    -moz-filter: blur(12px);

    -ms-filter: blur(12px);

    -o-filter: blur(12px);

    filter: blur(12px);

}

.CaseStudy {

    position: absolute;

    left: 0px;

    top: 25px;

    width: 1085px;

    height: 490px;

    background: rgba(255, 255, 255, 0.90);

    transform: scale(1.1);

