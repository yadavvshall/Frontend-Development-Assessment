        $(".clickToEnlarge").show();
        $(".small_expand").show();
        $(".small_expand1").show();
    } else {
        fnCompletion();
        enableNextBtn();
        $('.boxInst').show();
        $(".clickToEnlarge").show();
        $(".small_expand").show();
        $(".small_expand1").show();
        gCompPageArr.push("M1L0T1P25");
        fnAudioFinishItemComp();
        audplayer.pause();
        audplayer.currentTime = 0;
    }

}

var correctOptions = [1];

function bindClickableHotspot() {
    console.log("clicked");
    $('.option').addClass('aactive'); //.css({ 'cursor': 'pointer' });
    $('.option').off().on('click', function() {
		$('.option').removeClass('clicked');
		$(this).addClass('clicked');
		if ($('.option.clicked').length >= 1) {
			$('.cSubmitBtn').addClass('active');
			$('.cSubmitBtn.active').off().on('click', function() {
				submitClick();
			});
		} else {
			$('.cSubmitBtn').removeClass('active');
		}
    });
}

$('#topImgCnt2').hide();
function submitClick() {
    tinCanWrapper.sendStatementData("answered", "baby-jazmine-vital-Signs-assessment", "At 2 hours of age, Baby Jazmine is brought to the Special Care Nursery because of respiratory distress. Her vital signs are: Axillary temperature: 36.1°C, Heart rate: 172 beats per minute, Respiratory rate: 80 breaths per minute, Blood pressure: 45/28 (mean 34). These vital signs are? ");
    $('.cSubmitBtn.active').hide();
    console.log("clicked");
    enableNextBtn();
    fnDesiableNext(false);
    gCompPageArr.push("M1L0T1P25");
    var incorrectCount = 0; //, correctCount = 0;
    $('.option').each(function() {
        if ($(this).hasClass('clicked') && ($.inArray($(this).index(), correctOptions) == -1)) {
            $(this).addClass('wrong');
            incorrectCount++;
        }
        $(this).off(); //.removeClass('clicked');
    });

    $(correctOptions).each(function(i, val) {
        $('.option').eq(val).addClass('correct');
    });

    if (incorrectCount == 0) {
        fnPlaySectionAudio(0);
    } else {
        fnPlaySectionAudio(1);
    }
    $('.option').off().removeClass('aactive'); //.css({'cursor':'default'});
    $('.cSubmitBtn').removeClass('active');
	$('.rightCnt').show();
    $('.informationRight').show();
}

