            var ele = document.getElementsByTagName('div');

            for (var i = 0; i < ele.length; i++) {

                ele[i].setAttribute("unselectable", "on");

            }

            var n = isInArray("M1L0T1P59_6", gCompPageArr);

            if (n == true) {

                var _getSelectedOpt = assessmentObj.getSelectedOptionId(5);
