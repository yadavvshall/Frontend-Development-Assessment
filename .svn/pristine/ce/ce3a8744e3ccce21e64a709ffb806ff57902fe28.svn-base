                } else {
                    //incorrect_answer++
                    assessmentObj.wrongAnswerIncrement();
                    $(".cSubmitBtn").removeClass("active");
                    fnCompletion();


                }

                fnJumpToPageByID("M1L0T1P59_7");

            } else {



            }

        });
