        @-webkit-keyframes reducetime {
            0% {
                width: 0;
            }

            100% {
                width: 100%;
            }
        }

        @-webkit-keyframes zoomin {
            0% {
                transform: scale(1);
            }

            50% {
                transform: scale(1.2);
            }

            100% {
                transform: scale(1);
            }
        }

        @keyframes zoomin {
            0% {
                transform: scale(1);
            }

            50% {
                transform: scale(1.2);
            }

            100% {
                transform: scale(1);
            }
        }

        /*End of Zoom in Keyframes */
        /* Zoom out Keyframes */

        @-webkit-keyframes zoomout {
            0% {
                transform: scale(1);
            }

