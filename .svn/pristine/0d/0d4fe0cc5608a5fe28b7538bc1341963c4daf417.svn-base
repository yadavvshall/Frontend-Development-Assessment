.dragRegion {
    width: 262px;
    height: 404px;
    overflow: hidden;
    float: left;
    background: rgba(0, 0, 0, 0.2);
    border-radius: 5px;
    padding-top: 7px;
}

.drag {
    width: 230px;
    height: 121px;
    float: left;
    position: relative;
    background: rgb(255, 255, 255);
    margin: 6px 16px;
    border-radius: 5px;
    cursor: pointer;
}

.drag.disabled {
    cursor: default;
}

.dragRegion .drag.incorrect {
    box-shadow: 0 0 0 1px red;
}

.dragRegion .drag.incorrect:before {
    content: "";
    background: url('../../../pages/images/Topic01/T01P68/wrong.svg') no-repeat;
    position: absolute;
    width: 28px;
    height: 28px;
    right: 0;
}

.dragImage {
